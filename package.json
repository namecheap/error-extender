{
  "name": "@namecheap/error-extender",
  "version": "2.1.0",
  "description": "error-extender",
  "main": "main/ErrorExtender.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "nyc mocha '**/*.spec.js' --forbid-only --forbid-pending --reporter=\"mochawesome\" --reporter-options=\"reportDir=./test-results/mocha/,reportFilename=index.html\""
  },
  "files": [
    "main",
    "index.d.ts",
    "LICENSE.txt",
    "NOTICE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namecheap/error-extender.git"
  },
  "keywords": [
    "error",
    "extends",
    "extender",
    "error-extender",
    "exception",
    "jpbaking"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/namecheap/error-extender/issues"
  },
  "homepage": "https://github.com/namecheap/error-extender#readme",
  "devDependencies": {
    "jshint": "^2.13.6",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "watermarks": {
      "lines": [
        85,
        100
      ],
      "functions": [
        85,
        100
      ],
      "branches": [
        85,
        100
      ],
      "statements": [
        85,
        100
      ]
    },
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test-results",
      "**/*.spec.js"
    ],
    "reporter": [
      "text",
      "html",
      "cobertura"
    ],
    "cache": false,
    "all": true,
    "temp-directory": "./test-results/coverage/.tmp",
    "report-dir": "./test-results/coverage"
  }
}
